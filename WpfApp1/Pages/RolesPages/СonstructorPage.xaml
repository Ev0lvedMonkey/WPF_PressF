<Page x:Class="WpfApp1.Pages.RolesPages.СonstructorPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp1.Pages.RolesPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="СonstructorPage">

    <Grid Background="#f3cc8d">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="ToMaterialsAndComponentsListBtn" Content="Материалы и компоненты"
                    Margin="5" Click="ToMaterialsAndComponentsListBtn_Click"/>
            <Button x:Name="ToOrderBtn" Content="Заказы клиентов" Margin="5" Click="ToOrderBtn_Click"/>
        </StackPanel>
    </Grid>
</Page>
